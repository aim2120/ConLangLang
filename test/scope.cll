mystr = 'hello';
myotherstr = 'world';

sprint(mystr);
sprint(myotherstr);

myfunc = <:int> {
    sprint(mystr);
    sprint(myotherstr);

    myinnerfunc = <:int> {
        sprint(mystr);
        sprint(myotherstr);
    };

    myinnerfunc();

    mystr = 'good';
    myotherstr = 'bye';

    sprint(mystr);
    sprint(myotherstr);

    myotherinnerfunc = <:int> {
        sprint(mystr);
        sprint(myotherstr);
    };
    myinnerfunc();
    myotherinnerfunc();

    0;
};

myfunc();

mystr = 'different hello';
myotherstr = 'different world';

myfunc();

bang = '!';

add_bang = <string a, string s:string> {
    a ^ s ^ bang ^ ' ';
};

string_list = <string>['cat','dog','bird'];

s = lfold(add_bang, '', string_list);
sprint(s);


add_dict_bang = <string a, string k, string v:string> {
    a ^ k ^ v ^ bang;
};

strstr_dict = <string,string>{'a':'A', 'b':'B', 'c':'C'};

s = dfold(add_dict_bang, '', strstr_dict);
sprint(s);

add_bang = <string e:string>{
    e ^ bang ^ bang;
};

string_list_bang = lmap(add_bang, string_list);

s = lfold(<string a, string e:string>{ a ^ e ^ ' '; }, '', string_list_bang);
sprint(s);

add_dict_bang = <string k, string v:string> {
    v ^ bang ^ bang;
};

strstr_dict_bang = dmap(add_dict_bang, strstr_dict);

s = dfold(<string a, string k, string v:string>{ a ^ k ^ ':' ^ v ^ ' '; }, '', strstr_dict_bang);
sprint(s);
