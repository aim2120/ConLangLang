d = <int,int>{0:-0};
d2 = <string,string>{};

sprint((string)(dget(d,0)));

dadd(d,1,-1);

sprint((string)(dget(d,1)));
sprint((string)(dsize(d)));

f = <string a, string k, string v:string>{
    a ^ k ^ ':' ^ v ^ ' ';
};

dadd(d2,'a','A');
dadd(d2,'b','B');
dadd(d2,'c','C');
dadd(d2,'d','D');

sprint((string)(dmem(d2,'a')));
sprint((string)(dmem(d2,'z')));

s = dfold(f,'',d2);
sprint(s);
d2 = dremove(d2,'d');
s = dfold(f,'',d2);
sprint(s);

f2 = <int a, int k, int v:int>{
    if:int (k % 2 == 0) {
        sprint('even key: ' ^ ((string)k));
    } else {
        sprint('odd key: ' ^ ((string)k));
    };
    a + k;
};

f3 = <int a, int k, int v:int>{
    if:int (v % 2 == 0) {
        sprint('even value: ' ^ ((string)v));
    } else {
        sprint('odd value: ' ^ ((string)v));
    };
    a + v;
};

dadd(d,2,-2);
dadd(d,3,-3);
dadd(d,4,-4);

sprint((string)(dmem(d,2)));
sprint((string)(dmem(d,9)));

sprint((string)(dsize(d)));

keysum = dfold(f2,0,d);
valsum = dfold(f3,0,d);

sprint((string)keysum);
sprint((string)valsum);

